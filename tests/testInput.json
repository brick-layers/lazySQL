{
  "db": {
    "URI": "postgres://localhost:5432/das-brick-layer",
    "options": {
      "logging": false
    }
  },
  "models": {
    "User": {
      "name": {
        "type": "Sequelize.STRING"
      },
      "age": {
        "type": "Sequelize.INTEGER",
        "defaultValue": 20
      }
    },
    "Product": {
      "title": {
        "type": "Sequelize.STRING"
      },
      "quantity": {
        "type": "Sequelize.STRING",
        "allowNull": false,
        "defaultValue": 10
      }
    }
  },
  "associations": {
    "User": {
      "hasMany": {
        "Product": null
      }
    },
    "Product": {
      "belongsTo": {
        "User": { "as": "possession" }
      }
    }
  },
  "options": {}
}
